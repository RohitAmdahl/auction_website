  "version": "0.1.2",
  "description": "Auction website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RohitAmdahl/semster-project-2-.git"
  },
  "keywords": [
    "javascript_Sass_bootstrap_Api_online",
    "biding"
  ],
  "author": "Rohit kumar <rohit_designer@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RohitAmdahl/semster-project-2-/issues"
  },
  "homepage": "https://github.com/RohitAmdahl/semster-project-2-#readme",
  "dependencies": {
    "bootstrap": "^5.2.3"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "live-server": "^1.2.2",
    "prettier": "^2.8.3",
    "sass": "^1.58.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
